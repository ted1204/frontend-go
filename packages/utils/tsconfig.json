{
  "extends": "../../tsconfig.app.json",
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "rootDir": "src",
    "outDir": "../../build/utils",
    "noEmit": false, // Set to true to allow user preference; project references will not work with this setting.
    "tsBuildInfoFile": "../../build/utils/.tsbuildinfo"
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/locales/zh.ts"]
}
